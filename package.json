  },
  "homepage": "https://github.com/pet-booking/backend#readme",
  "devDependencies": {
    "eslint": "^4.13.1",
    "faker": "^4.1.0",
    "jest": "^22.0.3",
    "nodemon": "^1.13.3"
  }
}
