    "aws-sdk-mock": "^1.7.0",
    "eslint": "^4.15.0",
    "faker": "^4.1.0",
    "jest": "^22.0.4",
    "nodemon": "^1.14.8"
  },
  "dependencies": {
    "aws-sdk": "^2.177.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
