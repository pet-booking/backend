{
  "engines": {
    "node": "8.4.0"
  },
  "name": "pet-booking-backend",
  "version": "0.0.1",
  "description": "RESTful Pet Booking API for TLC Sitters",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "start": "node index.js",
    "watch": "nodemon index.js",
    "load-test": "wrk -t12 -c400 -d30s -s ./__test__/create-products.lua https://sail-point-backend.herokuapp.com/products",
    "test-watch": "jest --watch -i --coverage",
    "test-watch-inBand": "jest --watch --runInBand",
    "test-watch-lite": "jest --watch -i",
    "test": "jest -i --coverage",
    "dbonWin": "mkdir -p $HOME/db && mongod --dbpath $HOME/db",
    "dbon": "mkdir -p ./db && mongod --dbpath ./db",
    "dboff": "killall mongod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pet-booking/backend.git"
  },
